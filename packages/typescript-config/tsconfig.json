{
  "target": "es2015",
  "compilerOptions": {
    "target": "ESNext", // 或者其他适合的版本
    "module": "ESNext", // 或 "module": "ES6"
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "emitDeclarationOnly": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
